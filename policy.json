{
  "Version": "2012-10-17",
  "Statement": [{
      "Sid": "s3",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:PutObject"

      ],
      "Resource": [
        "arn:aws:s3:::entu-web-*",
        "arn:aws:s3:::entu-web-*/*"
      ]
    },
    {
      "Sid": "cloudfront",
      "Effect": "Allow",
      "Action": "cloudfront:CreateInvalidation",
      "Resource": "arn:aws:cloudfront::*:distribution/*"
    }
  ]
}
